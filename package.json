{
  "name": "luckystarry-log4ts-aliyun",
  "version": "1.0.0",
  "description": "一个简单的日志记录组件",
  "scripts": {
    "prebuild": "del-cli dist -f",
    "build": "tsc",
    "postbuild": "cp-cli ./src/sls.proto ./dist/sls.proto",
    "pretest": "del-cli .nyc_output coverage -f",
    "test": "nyc mocha",
    "testonly": "mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "gulp dev"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "include": [
      "src"
    ],
    "all": true
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuckyStarry/luckystarry-log4ts-aliyun.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuckyStarry/luckystarry-log4ts-aliyun/issues"
  },
  "homepage": "https://github.com/LuckyStarry/luckystarry-log4ts-aliyun#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.2",
    "chai": "^4.3.10",
    "coveralls": "^3.0.2",
    "cp-cli": "^2.0.0",
    "cross-env": "^6.0.0",
    "del-cli": "^2.0.0",
    "gulp": "^5.0.0",
    "mocha": "^10.2.0",
    "nyc": "^17.0.0",
    "should": "^13.2.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "@alicloud/log": "^1.1.1",
    "luckystarry-log4ts": "^1.0.1"
  }
}
